[Intel]
; Empty section.

[Intel.NTamd64.6.0]
; Empty section.


;+++++++++++++++++++++++ Windows 7 (x64) ++++++++++++++++++++++++++++++++++++++++++++++
[Intel.NTamd64.6.1.1]
